{
  "name": "clabsid/easy-laravel-package",
  "description": "Easy package for create repository, service, traits, view from artisan and generate custom ID",
  "type": "library",
  "keywords": [
    "laravel",
    "laravel-repository",
    "laravel-repository-service",
    "generate-custom-kode"
  ],
  "require": {
    "php": "^8.0",
    "spatie/laravel-package-tools": "1.15.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/clabsid/easy-laravel-package.git"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "EasyLaravelPackage\\": "src"
    }
  },
  "authors": [
    {
      "name": "clabsid",
      "email": "imho.production@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "EasyLaravelPackage\\EasyLaravelPackageProvider",
        "EasyLaravelPackage\\RepositoryServiceProvider"
      ],
      "aliases": {
        "EasyLaravelPackage": "EasyLaravelPackage\\EasyLaravelPackageFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
